<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.swatch {
	   width: 60px;
	   height: 60px;
	   display: block;
	   margin: auto;
	}
	
	.pad {
	   padding: 5px;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="row">
			<div class="col-sm-4 text-center {style.pad}">
				<span class="img-circle {style.swatch}" ui:field="elVibrant"/>
				<span class="h3">Vibrant</span>
			</div>
			<div class="col-sm-4 text-center {style.pad}">
				<span class="img-circle {style.swatch}" ui:field="elVibrantDark" />
				<span class="h3">Vibrant Dark</span>
			</div>
			<div class="col-sm-4 text-center {style.pad}">
				<span class="img-circle {style.swatch}" ui:field="elVibrantLight" />
				<span class="h3">Vibrant Light</span>
			</div>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="row text-center">
			<div class="col-sm-4">
				<span class="img-circle {style.swatch}" ui:field="elMuted" />
				<span class="h3">Muted</span>
			</div>
			<div class="col-sm-4 text-center {style.pad}">
				<span class="img-circle {style.swatch}" ui:field="elMutedDark" />
				<span class="h3">Muted Dark</span>
			</div>
			<div class="col-sm-4 text-center {style.pad}">
				<span class="img-circle {style.swatch}" ui:field="elMutedLight" />
				<span class="h3">Muted Light</span>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 